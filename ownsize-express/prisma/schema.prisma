generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id(map: "user_pk") @unique(map: "user_id_uindex") @default(autoincrement())
  name  String? @db.VarChar(10)
  email String? @db.VarChar(50)
}
